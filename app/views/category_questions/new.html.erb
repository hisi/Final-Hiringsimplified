<h1>New category_question</h1>

<% form_for(@category_question) do |f| %>
  <%= f.error_messages %>


<%=  f.hidden_field(:category_id, :cat_id) %>

  <p>
    <%= f.label :category_id %><br />
    <%= f.text_field :category_id %>
  </p>
  <p>
    <%= f.label :question %><br />
    <%= f.text_area :question %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', category_questions_path %>