<h1>New job_profile</h1>

<% form_for(@job_profile) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %>:<br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :description %>:<br />
    <%= f.text_area :description %>
  </p>

  <p>
    City  :<br /> <%= text_field :location ,:city %>
  </p>
  <p>
    State  :<br /> <%= text_field :location ,:state %>
  </p>
  <p>
    Country  :<br /> <%= text_field :location ,:country %>
  </p>



  <p>
  <%#= f.label :job_opening_id %><br />
  <%#= f.text_field :job_opening_id %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', job_profiles_path %>