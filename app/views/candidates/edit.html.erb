<%= javascript_include_tag :defaults %>
<script language="javascript">
  var curr_index = 0;
  var cals=new Array();

  function attach(obj) {
    var dhxCalendarData = {
        parent: obj,
        isAutoDraw: false,
        isYearEditable:true
    };
    cals[curr_index] = new dhtmlxCalendarObject(dhxCalendarData,true,{isYearEditable: true});
    cals[curr_index].setSkin("dhx_blue");
    cals[curr_index].setDateFormat("%m/%d/%Y");
    curr_index++;


}

</script>
<div class="candidate_signup_box" id="candidate">
<% form_for :candidate,@candidate, :url => candidates_path(@candidate), :html => { :method => 'put' } do |f| %>
  <%= render :partial => 'fields', :locals => { :f => f } %>
  <p><div class="right_box"><%= submit_tag "Update",:class=>"silver_button_blue" %></div></p>
<div class="spacer"></div>
<% end %>
</div>
